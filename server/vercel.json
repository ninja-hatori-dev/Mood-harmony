{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
  "routes": [
    {
      "src": "/api/register",
      "dest": "/app.py"
    },
    {
      "src": "/api/login",
      "dest": "/app.py"
    },
    {
      "src": "/api/recommendations",
      "dest": "/app.py"
    }
  ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
}